{
    "$schema": "../node_modules/wehub/config.schema.json",
    "apps": [
        {
            "name": "1-hello-world",
            "url": "/hub/wetouch/1-hello-world/",
            "path": "./apps/1-hello-world/"
        },
        {
            "name": "2-p5",
            "url": "/hub/wetouch/2-p5/",
            "path": "./apps/2-p5/"
        },
        {
            "name": "3-calculator",
            "url": "/hub/wetouch/3-calculator/",
            "path": "./apps/3-calculator/"
        },
        {
            "name": "4-quiz",
            "url": "/hub/wetouch/4-quiz/",
            "path": "./apps/4-quiz/"
        }
    ],
    "server": {
        "devOptions": {
            "buildExtensions": true
        }
    },
    "extensions": {
        "Electron": {
            "enabled": false
        },
        "Helloworld": {
            "enabled": true
        },
        "Logging": {
            "enabled": "$IN_PRODUCTION"
        },
        "WatchDog": {
            "logInfo": false
        }
    },
    "mode": "development"
}